{
    "name": "@perfyjs/perfy",
    "version": "1.0.0",
    "description": "The perfect companion for Benchpress Performance Toolkit",
    "main": "index.js",
    "bin": {
        "perfy": "./cli.js"
    },
    "scripts": {
        "--pretest": "webdriver-manager update",
        "test": "protractor protractor.conf.js",
        "posttest": "npm run perfy",
        "perfy": "node cli.js protractor.conf.js",
        "http": "http-server ./perf_reports -p4242 -a0.0.0.0 -c-1",
        "clean": "rimraf perf_reports/reports/*/**.json",
        "socket": "node ./server.js",
        "start": "npm run http"
    },
    "homepage": "https://github.com/perfyjs/perfy",
    "bugs": "https://github.com/perfyjs/perfy/issues",
    "author": "Wassim CHEGHAM <github@wassimchegham.com>",
    "engines": {
        "node": ">= 5.4.1 < 8"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/perfyjs/perfy.git"
    },
    "private": false,
    "license": "MIT",
    "dependencies": {
        "@angular/benchpress": "git@github.com:perfyjs/benchpress.git",
        "asyncawait": "1.0.6",
        "fs-extra": "3.0.0",
        "glob": "7.1.1",
        "http-server": "0.9.0",
        "optimist": "^0.6.1",
        "protractor": "~5.1.0",
        "socket.io": "1.7.3"
    },
    "devDependencies": {
        "rimraf": "2.6.1"
    }
}